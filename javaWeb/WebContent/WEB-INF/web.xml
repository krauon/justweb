<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>greeting</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>		
	</welcome-file-list>
	<servlet>
		<servlet-name>byget</servlet-name>
		<servlet-class>com.javaWeb.basic.BasicHttps</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>byget</servlet-name>
		<url-pattern>/byget.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>bypost</servlet-name>
		<servlet-class>com.javaWeb.basic.BasicHttps</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>bypost</servlet-name>
		<url-pattern>/bypost.do</url-pattern>
	</servlet-mapping>
</web-app>